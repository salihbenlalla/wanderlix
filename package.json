{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "pnpm run init.db && qwik build > /dev/null",
    "build.local": "pnpm run fill.db.local && qwik build",
    "build.client": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dbinit": "wrangler -c ./wrangler.toml d1 execute DB --local --file ./schema.sql",
    "deploy": "wrangler pages deploy ./dist",
    "dev": "vite --mode ssr --host",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist --d1 DB --local --compatibility-flags=nodejs_compat",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "wrangler": "wrangler",
    "migrate": "wrangler -c ./wrangler.toml d1 execute DB --local --file ./schema.sql",
    "import.db": "pnpm run empty.db && npx wrangler d1 execute DB --remote --file=/home/salih/D/programming/Qwik/travel3/travel2/.wrangler/state/v3/d1/miniflare-D1DatabaseObject/db.sql --batch-size=5000",
    "empty.db": "npx wrangler d1 execute DB --remote --file=empty_db.sql",
    "export.db": "npx wrangler d1 export DB --remote --output=./database.sql",
    "download.db": "rm -f ./database.sql && npx wrangler d1 export DB --remote --output=./database.sql",
    "fill.db": "wrangler -c ./wrangler.toml d1 execute DB --local --file=empty_db.sql --json > /dev/null && wrangler -c ./wrangler.toml d1 execute DB --local --file ./database.sql --json > /dev/null",
    "fill.db.local": "wrangler -c ./wrangler.toml d1 execute DB --local --file=empty_db.sql && wrangler -c ./wrangler.toml d1 execute DB --local --file ./database.sql",
    "init.db": "pnpm run download.db && pnpm run fill.db"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.4.0",
    "@mdx-js/mdx": "^2.3.0",
    "@modular-forms/qwik": "^0.24.0",
    "@salihbenlalla/qwik-city": "^1.4.0",
    "@svgx/vite-plugin-qwik": "^1.0.1",
    "@types/dateformat": "^5.0.2",
    "@types/eslint": "^8.56.6",
    "@types/hast": "^3.0.4",
    "@types/jump.js": "^1.0.6",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/scriptjs": "^0.0.5",
    "@types/unist": "^3.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "better-sqlite3": "^9.5.0",
    "dateformat": "^5.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "^1.5.2",
    "jump.js": "^1.0.2",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdx": "^0.3.1",
    "motion": "^10.17.0",
    "prettier": "^3.2.5",
    "qwik-image": "^0.0.13",
    "scriptjs": "^2.5.9",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "undici": "*",
    "unified": "^10.1.2",
    "uuid": "^9.0.1",
    "valibot": "0.30.0",
    "vite": "5.2.11",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.76.0"
  },
  "dependencies": {
    "@miniflare/d1": "^2.14.2",
    "@miniflare/shared": "^2.14.2",
    "miniflare": "^3.20240405.2"
  }
}
